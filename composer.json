{
    "name": "eko/feedbundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle to build RSS feeds from entities",
    "keywords": ["feed", "bundle", "rss", "atom"],
    "homepage": "http://github.com/eko/FeedBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Composieux",
            "email": "vincent.composieux@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/framework-bundle": "^4.0|^5.0",
        "symfony/translation": "^4.0|^5.0",
        "laminas/laminas-feed": "^2.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.7",
        "phpunit/phpunit": "^8.5 || ^9.5"
    },
    "suggest": {
        "doctrine/orm": "~2.7"
    },
    "autoload": {
        "psr-4": { "Eko\\FeedBundle\\": "" }
    }
}
